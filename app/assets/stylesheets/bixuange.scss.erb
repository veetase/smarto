.card{
  width: 220px;
  height: 220px;
  display: inline-block;
  padding: 10px;
  cursor: pointer;
}

.card .front{
  width: 100%;
  height: 100%;
}

.card div{
  backface-visibility: hidden;
}

.card .back{
  background-color: white;
  -webkit-border-radius: 120px;
  -moz-border-radius: 120px;
  border-radius: 120px;
  padding: 50px 0;
  border: 1px;
  border-color: black;
}

.card div.short_title{
  height: 24px;
  line-height: 24px;
  font-size: 24px;
}

.card div.short_title.middle{
  height: 19px;
  font-size: 19px;
  line-height: 19px;
}

.card div.name, .card div.short_name{
  margin-top: 20px;
  height: 60px;
  line-height: 40px;
}


.card div.long_title{
  height: 14px;
  font-size: 14px;
  line-height: 14px;
}

.card div.short_name{
  font-size: 60px;
  word-spacing: 0.2em;
}

.card .short_title::before, .card .long_title::before{
  content: "";
  float: left;
  height: 100%;
  width: 10px;
  @include brackets-sprite(bracket2Left);
  @include resize-sprite($brackets-sprites, bracket2Left, 40);
}

.card .short_title::before, .card .long_title::before{
  margin-left: 50px;
}

.card .short_title::after, .card .long_title::after{
  margin-right: 50px;
}

.card .short_title::after, .card .long_title::after{
  content: "";
  float: right;
  height: 100%;
  width: 10px;
  @include brackets-sprite(bracket2Right);
  @include resize-sprite($brackets-sprites, bracket2Right, 40);
}

.card div.name{
  font-size: 40pt;
}

.card div.name_en{
  font-size: 46px;
  margin-top: 20px;
  line-height: 35px;
}

.card div.en_small{
  font-size: 16pt;
  line-height: 16px;
}

.card .en_very_small{
  font-size: 15px;
  margin-top: 5px;
}

.card .en_tall{
  font-size: 15px;
  margin-top: 5px;
}

.card div.desc{
  font-size: 22px;
  height: 22px;
  line-height: 22px;
}

.card div.long_desc{
  font-size: 20px;
  height: 20px;
  width: 90%;
  margin: 0 auto;
  line-height: 20px;
}

.card:nth-of-type(1) .front {
  @include members-sprite(member_1);
}
.card:nth-of-type(2) .front {
  @include members-sprite(member_2);
}
.card:nth-of-type(3) .front{
  @include members-sprite(member_3);
}
.card:nth-of-type(4) .front {
  @include members-sprite(member_4);
}
.card:nth-of-type(5) .front {
  @include members-sprite(member_5);
}
.card:nth-of-type(6) .front{
  @include members-sprite(member_6);
}
.card:nth-of-type(7) .front {
  @include members-sprite(member_7);
}
.card:nth-of-type(8) .front{
  @include members-sprite(member_8);
}
.card:nth-of-type(9) .front{
  @include members-sprite(member_9);
}
